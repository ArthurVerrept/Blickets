    "chai-as-promised": "7.1.1",
    "chai-bignumber": "3.0.0",
    "core-js": "^3.6.5",
    "mocha": "^8.3.2",
    "regenerator-runtime": "^0.13.7",
    "solc": "^0.8.2",
    "truffle": "5.0.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuex": "^3.6.2",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "web3": "1.0.0-beta.55"
  },
  "devDependencies": {
    "@babel/register": "^7.13.8",
