{
  "name": "eth-marketplace",
  "version": "0.1.0",
  "description": "An Ethereum Marketplace",
  "author": "gregory@dappuniversity.com",
  "scripts": {
    "serve:local": "NODE_ENV=true vue-cli-service serve",
    "serve:rinkeby": "NODE_ENV=false vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "truffle test",
    "deploy:rinkeby": "truffle migrate --network rinkeby",
